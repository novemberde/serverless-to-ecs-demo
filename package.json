{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/spec/index.spec.ts",
    "start": "npm-run-all -p build:dev start:devServer",
    "build:dev": "node scripts/build.js",
    "build:test": "webpack --config config/webpack.config.test.js",
    "build:dockerImage": "webpack --config config/webpack.config.dev.js",
    "start:devServer": "nodemon bin/main.js --watch bin",
    "deploy": "serverless deploy"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.22",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.0.57",
    "@types/should": "^13.0.0",
    "@types/supertest": "^2.0.4",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.0",
    "npm-run-all": "^4.1.2",
    "serverless": "^1.26.0",
    "serverless-webpack": "^4.0.0",
    "should": "^13.2.1",
    "source-map-support": "^0.5.3",
    "supertest": "^3.0.0",
    "ts-loader": "^2.3.7",
    "ts-node": "^5.0.0",
    "typescript": "^2.5.2",
    "webpack": "^3.11.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "aws-serverless-express": "^3.1.3",
    "express": "^4.16.2",
    "lodash": "^4.17.5"
  }
}
